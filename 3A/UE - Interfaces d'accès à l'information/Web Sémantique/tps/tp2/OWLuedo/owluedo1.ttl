###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  https://w3id.org/cluedo4KG/onto#aPourCauseDeDécès
cluedonto:aPourCauseDeDécès rdf:type owl:ObjectProperty ;
                            rdfs:domain cluedonto:victim ;
                            rdfs:range cluedonto:causeofdeath ;
                            rdfs:label "a pour cause de décès" .


###  https://w3id.org/cluedo4KG/onto#aPourPieceVoisine
cluedonto:aPourPieceVoisine rdf:type owl:ObjectProperty ,
                                     owl:SymmetricProperty ,
                                     owl:IrreflexiveProperty ;
                            rdfs:domain cluedonto:Piece ;
                            rdfs:range cluedonto:Piece ;
                            rdfs:label "a pour pièce voisine"@fr ,
                                       "has as neighbour room"@en .


###  https://w3id.org/cluedo4KG/onto#aTué
cluedonto:aTué rdf:type owl:ObjectProperty ;
               owl:inverseOf cluedonto:aÉtéTuéPar ;
               rdfs:domain cluedonto:Personne ;
               rdfs:range cluedonto:victim ;
               rdfs:label "a tué"@fr .


###  https://w3id.org/cluedo4KG/onto#a_causé_la_mort
cluedonto:a_causé_la_mort rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf owl:topObjectProperty ;
                          rdfs:domain cluedonto:causeofdeath ;
                          rdfs:range cluedonto:victim ;
                          rdfs:label "a causé la mort"@fr .


###  https://w3id.org/cluedo4KG/onto#aÉtéTuéPar
cluedonto:aÉtéTuéPar rdf:type owl:ObjectProperty ;
                     rdfs:domain cluedonto:victim ;
                     rdfs:range cluedonto:murderer ;
                     rdfs:label "a été tué par"@fr .


###  https://w3id.org/cluedo4KG/onto#contient_zone
cluedonto:contient_zone rdf:type owl:ObjectProperty ;
                        rdfs:domain cluedonto:Piece ;
                        rdfs:range cluedonto:zone ;
                        rdfs:label "pièce contient zone"@fr .


###  https://w3id.org/cluedo4KG/onto#estActuellementDansLaPièce
cluedonto:estActuellementDansLaPièce rdf:type owl:ObjectProperty ;
                                     rdfs:subPropertyOf cluedonto:estOuAÉtéDansLaPièce ;
                                     rdfs:domain cluedonto:Personne ;
                                     rdfs:range cluedonto:Piece ;
                                     rdfs:label "est actuellement dans la pièce"@fr .


###  https://w3id.org/cluedo4KG/onto#estActuellementEnPossessionDe
cluedonto:estActuellementEnPossessionDe rdf:type owl:ObjectProperty ;
                                        rdfs:subPropertyOf cluedonto:estOuAÉtéEnPossessionDe ;
                                        rdfs:domain cluedonto:Personne ;
                                        rdfs:range cluedonto:Objet ;
                                        rdfs:label "est actuellement en possession de"@fr .


###  https://w3id.org/cluedo4KG/onto#estCentreDe
cluedonto:estCentreDe rdf:type owl:ObjectProperty ;
                      rdfs:domain cluedonto:Objet ;
                      rdfs:range cluedonto:zone .


###  https://w3id.org/cluedo4KG/onto#estOuAÉtéDansLaPièce
cluedonto:estOuAÉtéDansLaPièce rdf:type owl:ObjectProperty ;
                               rdfs:subPropertyOf cluedonto:est_ou_a_été_dans_la_place ;
                               rdfs:domain cluedonto:Personne ;
                               rdfs:range cluedonto:Piece ;
                               rdfs:label "est ou a été dans la pièce"@fr .


###  https://w3id.org/cluedo4KG/onto#estOuAÉtéEnPossessionDe
cluedonto:estOuAÉtéEnPossessionDe rdf:type owl:ObjectProperty ;
                                  rdfs:domain cluedonto:Personne ;
                                  rdfs:range cluedonto:Objet ;
                                  rdfs:label "est ou a été en possession de"@fr .


###  https://w3id.org/cluedo4KG/onto#est_actuellement_dans_la_place
cluedonto:est_actuellement_dans_la_place rdf:type owl:ObjectProperty ;
                                         rdfs:subPropertyOf cluedonto:est_ou_a_été_dans_la_place ;
                                         rdfs:domain cluedonto:Personne ;
                                         rdfs:range cluedonto:place ;
                                         rdfs:label "est actuellement dans la place"@fr .


###  https://w3id.org/cluedo4KG/onto#est_attribuable_à_objet_mobile
cluedonto:est_attribuable_à_objet_mobile rdf:type owl:ObjectProperty ;
                                         rdfs:subPropertyOf cluedonto:aPourCauseDeDécès ;
                                         owl:inverseOf cluedonto:objet_mobile_attribué_à ;
                                         rdf:type owl:InverseFunctionalProperty ;
                                         rdfs:domain cluedonto:causeofdeath ;
                                         rdfs:range cluedonto:mobile ;
                                         rdfs:label "est attribuable à objet mobile" .


###  https://w3id.org/cluedo4KG/onto#est_ou_a_été_dans_la_place
cluedonto:est_ou_a_été_dans_la_place rdf:type owl:ObjectProperty ;
                                     rdfs:domain cluedonto:Personne ;
                                     rdfs:range cluedonto:place ;
                                     owl:propertyChainAxiom ( cluedonto:est_ou_a_été_dans_la_place
                                                              cluedonto:contient_zone
                                                            ) ,
                                                            ( cluedonto:est_ou_a_été_dans_la_place
                                                              cluedonto:floorContientPièce
                                                            ) ,
                                                            ( cluedonto:est_ou_a_été_dans_la_place
                                                              cluedonto:maison_contient_étage
                                                            ) ;
                                     rdfs:label "est ou a été dans la place"@fr .


###  https://w3id.org/cluedo4KG/onto#floorContientPièce
cluedonto:floorContientPièce rdf:type owl:ObjectProperty ;
                             rdfs:subPropertyOf cluedonto:locatedIn ;
                             owl:inverseOf cluedonto:pièce_dans_floor ;
                             rdfs:domain cluedonto:floor ;
                             rdfs:range cluedonto:Piece ;
                             rdfs:label "étage contient pièce"@fr .


###  https://w3id.org/cluedo4KG/onto#floor_dans_maison
cluedonto:floor_dans_maison rdf:type owl:ObjectProperty ;
                            rdfs:subPropertyOf cluedonto:locatedIn ;
                            owl:inverseOf cluedonto:maison_contient_étage ;
                            rdfs:domain cluedonto:floor ;
                            rdfs:range cluedonto:Maison ;
                            rdfs:label "étage dans maison"@fr .


###  https://w3id.org/cluedo4KG/onto#locatedIn
cluedonto:locatedIn rdf:type owl:ObjectProperty ,
                             owl:TransitiveProperty ;
                    rdfs:domain cluedonto:place ;
                    rdfs:range cluedonto:place ;
                    rdfs:label "place dans place"@fr .


###  https://w3id.org/cluedo4KG/onto#maisonContientPiece
cluedonto:maisonContientPiece rdf:type owl:ObjectProperty ;
                              rdfs:subPropertyOf cluedonto:locatedIn ;
                              owl:inverseOf cluedonto:pieceDansMaison ;
                              rdfs:domain cluedonto:Maison ;
                              rdfs:range cluedonto:Piece ;
                              rdfs:label "house contains room"@en ,
                                         "maison contient pièce"@fr .


###  https://w3id.org/cluedo4KG/onto#maison_contient_étage
cluedonto:maison_contient_étage rdf:type owl:ObjectProperty ;
                                rdfs:subPropertyOf cluedonto:locatedIn ;
                                rdfs:domain cluedonto:Maison ;
                                rdfs:range cluedonto:floor ;
                                rdfs:label "maison contient étage"@fr .


###  https://w3id.org/cluedo4KG/onto#objetDansPiece
cluedonto:objetDansPiece rdf:type owl:ObjectProperty ;
                         owl:inverseOf cluedonto:pieceContientObjet ;
                         rdfs:domain cluedonto:Objet ;
                         rdfs:range cluedonto:Piece ;
                         rdfs:label "object in room"@en ,
                                    "objet dans pièce"@fr .


###  https://w3id.org/cluedo4KG/onto#objet_mobile_attribué_à
cluedonto:objet_mobile_attribué_à rdf:type owl:ObjectProperty ;
                                  rdfs:label "objet mobile attribué à"@fr .


###  https://w3id.org/cluedo4KG/onto#organise_autour
cluedonto:organise_autour rdf:type owl:ObjectProperty ;
                          rdfs:domain cluedonto:zone ;
                          rdfs:range cluedonto:immobile ;
                          rdfs:label "organisé autour"@fr .


###  https://w3id.org/cluedo4KG/onto#personneDansPiece
cluedonto:personneDansPiece rdf:type owl:ObjectProperty ;
                            owl:inverseOf cluedonto:pieceContientPersonne ;
                            rdfs:domain cluedonto:Personne ;
                            rdfs:range cluedonto:Piece ;
                            owl:propertyChainAxiom ( cluedonto:estActuellementEnPossessionDe
                                                     cluedonto:objetDansPiece
                                                   ) ;
                            rdfs:label "person in room"@en ,
                                       "personne dans pièce"@fr .


###  https://w3id.org/cluedo4KG/onto#pieceContientObjet
cluedonto:pieceContientObjet rdf:type owl:ObjectProperty ;
                             rdfs:domain cluedonto:Piece ;
                             rdfs:range cluedonto:Objet ;
                             owl:propertyChainAxiom ( cluedonto:contient_zone
                                                      cluedonto:organise_autour
                                                    ) ;
                             rdfs:label "pièce contient objet"@fr ,
                                        "room contains object"@en .


###  https://w3id.org/cluedo4KG/onto#pieceContientPersonne
cluedonto:pieceContientPersonne rdf:type owl:ObjectProperty ;
                                rdfs:domain cluedonto:Piece ;
                                rdfs:range cluedonto:Personne ;
                                rdfs:label "pièce contient personne"@fr ,
                                           "room contains person"@en .


###  https://w3id.org/cluedo4KG/onto#pieceDansMaison
cluedonto:pieceDansMaison rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf cluedonto:locatedIn ;
                          rdfs:domain cluedonto:place ;
                          rdfs:range cluedonto:place ;
                          rdfs:label "pièce dans maison"@fr ,
                                     "room in house"@en .


###  https://w3id.org/cluedo4KG/onto#pièce_dans_floor
cluedonto:pièce_dans_floor rdf:type owl:ObjectProperty ;
                           rdfs:subPropertyOf cluedonto:locatedIn ;
                           rdfs:domain cluedonto:Piece ;
                           rdfs:range cluedonto:floor ;
                           rdfs:label "pièce dans étage"@fr .


###  https://w3id.org/cluedo4KG/onto#utilisé_pour_tuer
cluedonto:utilisé_pour_tuer rdf:type owl:ObjectProperty ;
                            rdfs:domain cluedonto:Objet ;
                            rdfs:range cluedonto:Personne ;
                            rdfs:label "utilisé pour tuer"@fr .


###  https://w3id.org/cluedo4KG/onto#victime_dans_lieu_du_crime
cluedonto:victime_dans_lieu_du_crime rdf:type owl:ObjectProperty ;
                                     rdfs:subPropertyOf owl:topObjectProperty .


#################################################################
#    Data properties
#################################################################

###  https://w3id.org/cluedo4KG/onto#a_été_morte_depuis
cluedonto:a_été_morte_depuis rdf:type owl:DatatypeProperty ;
                             rdfs:domain cluedonto:victim ;
                             rdfs:range xsd:int ;
                             rdfs:label "a été morte depuis"@fr .


###  https://w3id.org/cluedo4KG/onto#estVivant
cluedonto:estVivant rdf:type owl:DatatypeProperty ;
                    rdfs:domain cluedonto:Personne ;
                    rdfs:range xsd:boolean ;
                    rdfs:comment "indicates whether a person is alive (True) or dead (False)"@en ,
                                 "indique si une personne est vivante (True) ou morte (False)"@fr ;
                    rdfs:label "est vivant"@fr ,
                               "is alive"@en .


###  https://w3id.org/cluedo4KG/onto#produit_a_pour_densité
cluedonto:produit_a_pour_densité rdf:type owl:DatatypeProperty ;
                                 rdfs:domain cluedonto:product ;
                                 rdfs:range xsd:float ;
                                 rdfs:label "produit a pour densité"@fr .


#################################################################
#    Classes
#################################################################

###  https://w3id.org/cluedo4KG/onto#Maison
cluedonto:Maison rdf:type owl:Class ;
                 rdfs:subClassOf cluedonto:place ;
                 owl:disjointWith cluedonto:floor ;
                 rdfs:comment "house"@en ;
                 rdfs:label "maison"@fr .


###  https://w3id.org/cluedo4KG/onto#Objet
cluedonto:Objet rdf:type owl:Class ;
                rdfs:label "object"@en ,
                           "objet"@fr .


###  https://w3id.org/cluedo4KG/onto#Personne
cluedonto:Personne rdf:type owl:Class ;
                   rdfs:subClassOf [ rdf:type owl:Class ;
                                     owl:unionOf ( cluedonto:innocent
                                                   cluedonto:murderer
                                                   cluedonto:victim
                                                 )
                                   ] ;
                   rdfs:label "person"@en ,
                              "personne"@fr .


###  https://w3id.org/cluedo4KG/onto#Piece
cluedonto:Piece rdf:type owl:Class ;
                rdfs:subClassOf cluedonto:place ;
                owl:disjointWith cluedonto:floor ;
                rdfs:label "pièce"@fr ,
                           "room"@en .


###  https://w3id.org/cluedo4KG/onto#assassin
cluedonto:assassin rdf:type owl:Class ;
                   rdfs:subClassOf cluedonto:murderer ;
                   owl:disjointWith cluedonto:killer .


###  https://w3id.org/cluedo4KG/onto#blunt
cluedonto:blunt rdf:type owl:Class ;
                owl:equivalentClass [ rdf:type owl:Restriction ;
                                      owl:onProperty cluedonto:objet_mobile_attribué_à ;
                                      owl:someValuesFrom cluedonto:bluntObjectInjury
                                    ] ;
                rdfs:subClassOf cluedonto:weapon ;
                rdfs:label "contondant"@fr .


###  https://w3id.org/cluedo4KG/onto#bluntObjectInjury
cluedonto:bluntObjectInjury rdf:type owl:Class ;
                            owl:equivalentClass [ rdf:type owl:Restriction ;
                                                  owl:onProperty cluedonto:est_attribuable_à_objet_mobile ;
                                                  owl:someValuesFrom cluedonto:blunt
                                                ] ;
                            rdfs:subClassOf cluedonto:injury ;
                            rdfs:label "blessure par objet contondant"@fr .


###  https://w3id.org/cluedo4KG/onto#bulletInjury
cluedonto:bulletInjury rdf:type owl:Class ;
                       owl:equivalentClass [ rdf:type owl:Restriction ;
                                             owl:onProperty cluedonto:a_causé_la_mort ;
                                             owl:someValuesFrom cluedonto:tué_par_balle
                                           ] ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty cluedonto:est_attribuable_à_objet_mobile ;
                                             owl:someValuesFrom cluedonto:firearm
                                           ] ;
                       rdfs:subClassOf cluedonto:injury ;
                       rdfs:label "blessure par balle"@fr .


###  https://w3id.org/cluedo4KG/onto#causeofdeath
cluedonto:causeofdeath rdf:type owl:Class ;
                       owl:equivalentClass [ rdf:type owl:Restriction ;
                                             owl:onProperty cluedonto:a_causé_la_mort ;
                                             owl:someValuesFrom cluedonto:victim
                                           ] ;
                       rdfs:label "cause de la mort"@fr .


###  https://w3id.org/cluedo4KG/onto#drowning
cluedonto:drowning rdf:type owl:Class ;
                   rdfs:subClassOf cluedonto:causeofdeath ;
                   rdfs:label "noyade"@fr .


###  https://w3id.org/cluedo4KG/onto#firearm
cluedonto:firearm rdf:type owl:Class ;
                  owl:equivalentClass [ rdf:type owl:Restriction ;
                                        owl:onProperty cluedonto:objet_mobile_attribué_à ;
                                        owl:someValuesFrom cluedonto:bulletInjury
                                      ] ;
                  rdfs:subClassOf cluedonto:weapon ;
                  rdfs:label "arme à feu"@fr .


###  https://w3id.org/cluedo4KG/onto#floor
cluedonto:floor rdf:type owl:Class ;
                rdfs:subClassOf cluedonto:place ;
                rdfs:label "étage"@fr .


###  https://w3id.org/cluedo4KG/onto#immobile
cluedonto:immobile rdf:type owl:Class ;
                   rdfs:subClassOf cluedonto:Objet .


###  https://w3id.org/cluedo4KG/onto#injury
cluedonto:injury rdf:type owl:Class ;
                 rdfs:subClassOf cluedonto:causeofdeath ;
                 rdfs:label "blessure"@fr .


###  https://w3id.org/cluedo4KG/onto#innocent
cluedonto:innocent rdf:type owl:Class ;
                   rdfs:subClassOf cluedonto:Personne .


###  https://w3id.org/cluedo4KG/onto#killer
cluedonto:killer rdf:type owl:Class ;
                 rdfs:subClassOf cluedonto:murderer ;
                 rdfs:label "tueur"@fr .


###  https://w3id.org/cluedo4KG/onto#lieuDuCrime
cluedonto:lieuDuCrime rdf:type owl:Class ;
                      owl:equivalentClass [ rdf:type owl:Restriction ;
                                            owl:onProperty cluedonto:pieceContientPersonne ;
                                            owl:someValuesFrom cluedonto:victim
                                          ] ;
                      rdfs:subClassOf cluedonto:Piece ;
                      rdfs:label "lieu du crime"@fr .


###  https://w3id.org/cluedo4KG/onto#liquid
cluedonto:liquid rdf:type owl:Class ;
                 owl:equivalentClass [ rdf:type owl:Restriction ;
                                       owl:onProperty cluedonto:produit_a_pour_densité ;
                                       owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                            owl:onDatatype xsd:float ;
                                                            owl:withRestrictions ( [ xsd:maxExclusive "2.5"^^xsd:float
                                                                                   ]
                                                                                 )
                                                          ]
                                     ] ;
                 rdfs:subClassOf cluedonto:product ;
                 owl:disjointWith cluedonto:powder ;
                 rdfs:label "liquide"@fr .


###  https://w3id.org/cluedo4KG/onto#mobile
cluedonto:mobile rdf:type owl:Class ;
                 rdfs:subClassOf cluedonto:Objet .


###  https://w3id.org/cluedo4KG/onto#murderer
cluedonto:murderer rdf:type owl:Class ;
                   owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                owl:onProperty cluedonto:estOuAÉtéEnPossessionDe ;
                                                                owl:someValuesFrom cluedonto:firearm
                                                              ]
                                                              [ rdf:type owl:Restriction ;
                                                                owl:onProperty cluedonto:est_ou_a_été_dans_la_place ;
                                                                owl:someValuesFrom cluedonto:lieuDuCrime
                                                              ]
                                                            ) ;
                                         rdf:type owl:Class
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty cluedonto:aTué ;
                                         owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                         owl:onClass cluedonto:Personne
                                       ] ;
                   rdfs:subClassOf cluedonto:Personne ;
                   rdfs:label "Murderer"@en ,
                              "meurtrier"@fr .


###  https://w3id.org/cluedo4KG/onto#place
cluedonto:place rdf:type owl:Class .


###  https://w3id.org/cluedo4KG/onto#poisoning
cluedonto:poisoning rdf:type owl:Class ;
                    owl:equivalentClass [ rdf:type owl:Restriction ;
                                          owl:onProperty cluedonto:est_attribuable_à_objet_mobile ;
                                          owl:someValuesFrom cluedonto:product
                                        ] ;
                    rdfs:subClassOf cluedonto:causeofdeath ;
                    rdfs:label "empoisonnement"@fr .


###  https://w3id.org/cluedo4KG/onto#powder
cluedonto:powder rdf:type owl:Class ;
                 owl:equivalentClass [ rdf:type owl:Restriction ;
                                       owl:onProperty cluedonto:produit_a_pour_densité ;
